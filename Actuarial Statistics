      "cell_type": "code",
      "source": [
        "def partido(pais):\n",
        "  i=0\n",
        "  x= random.uniform(0,1)\n",
        "  nrows=len(frecuencias(df,pais))\n",
        "  while i< nrows:\n",
        "    if ( x < frecuencias(df,pais).iat[i,2]):\n",
        "      #print(pais,'hizo', frecuencias(df,pais).iat[0,0] ,'gol')\n",
        "      goles = frecuencias(df,pais).iat[i,0]\n",
        "      return int(goles)\n",
        "    else:\n",
        "       i += 1"
      ],
      "metadata": {
        "id": "tttjDuj_SzOz"
      },
      "execution_count": 34,
      "outputs": []
    },
