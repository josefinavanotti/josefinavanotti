              "          const element = document.querySelector('#df-aac911b2-9391-4aa8-b632-7f9667c14d68');\n",
              "          const dataTable =\n",
              "            await google.colab.kernel.invokeFunction('convertToInteractive',\n",
              "                                                     [key], {});\n",
