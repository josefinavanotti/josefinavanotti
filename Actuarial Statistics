    {
      "cell_type": "code",
      "execution_count": 145,
      "metadata": {
        "id": "4zBUDvAQ6bJ1",
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "outputId": "146585f1-d2c9-4474-b1cb-4f7e58b2ffef"
